{
  "name": "vue3-boilerplate-library",
  "version": "0.0.3",
  "description": "A vue3 library project base. Intended to quickstart your library development without all the configuration hassle.",
  "main": "./dist/vue3-boilerplate-library.umd.js",
  "module": "./dist/vue3-boilerplate-library.es.js",
  "types": "./dist/types/library.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hexagon06/vue3-boilerplate-library.git"
  },
  "keywords": [
    "vue",
    "vue3",
    "storybook",
    "rollup",
    "typescript",
    "vite"
  ],
  "author": "Jasper Saaltink",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/hexagon06/vue3-boilerplate-library/issues"
  },
  "homepage": "https://github.com/hexagon06/vue3-boilerplate-library#readme",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/vue3-boilerplate-library.umd.js",
      "require": "./dist/vue3-boilerplate-library.es.js"
    }
  },
  "scripts": {
    "build": "vite build && vue-tsc --emitDeclarationOnly",
    "preview": "vite preview",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "vue": "^3.2.25"
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@storybook/addon-actions": "^6.5.9",
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/addon-interactions": "^6.5.9",
    "@storybook/addon-links": "^6.5.9",
    "@storybook/builder-vite": "^0.1.38",
    "@storybook/testing-library": "^0.0.13",
    "@storybook/vue3": "^6.5.9",
    "@types/node": "^18.0.3",
    "@vitejs/plugin-vue": "^2.3.3",
    "babel-loader": "^8.2.5",
    "rollup-plugin-import-css": "^3.0.3",
    "storybook": "^6.5.9",
    "typescript": "^4.5.4",
    "vite": "^2.9.13",
    "vue-loader": "^16.8.3",
    "vue-tsc": "^0.38.2"
  }
}
